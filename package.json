{
  "name": "la-crosse-to-wunderground-pws",
  "description": "AWS service that fetches weather observations from La Crosse Alerts Mobile and submits them to a Weather Underground PWS",
  "version": "0.1.0",
  "author": "Mark Wainwright",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markwainwright/la-crosse-to-wunderground-pws.git"
  },
  "scripts": {
    "deploy": "npm run test && NODE_PRESERVE_SYMLINKS=1 serverless deploy",
    "invoke": "serverless invoke -f reader",
    "start": "ts-node run.ts",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "windrose": "2.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.25",
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.7",
    "aws-sdk": "2.437.0",
    "jest": "^24.8.0",
    "serverless": "^1.42.3",
    "serverless-plugin-typescript": "^1.1.7",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
